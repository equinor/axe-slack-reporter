{
    "root": true,
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "prettier"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 12
    },
    "rules": {
      "no-unused-vars": [
        "warn"
      ],
      "semi": ["error", "never"],
      // This rule was so noicy so let's turn it off and see how that works
      "import/newline-after-import": ["error"],
      "react/prop-types": "off"
    }
  }
  